<!DOCTYPE html>
<html {{function('language_attributes')}}>
<head>

    {{function('theme_comment')}}

    <title>{{function('bloginfo', 'wpseo_the_title')}}</title>
    <meta charset="{{ site.charset }}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{function('do_action', 'wpseo_the_desc')}}">
    <meta name="robots" content="index,follow">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link type="text/css" rel="stylesheet" href="{{function('theme_css_path')}}bundle.min.css">
    <link rel="alternate" type="application/rss+xml" title="mindtwo News Feed (RSS2)" href="{{function('bloginfo', 'rss2_url')}}">
</head>
<body class="{{ body_class }}">
    <div id="site">
        <header id="site__header" class="header clearfix" itemscope itemtype="http://schema.org/Organization">
            <div class="container">
                {% include 'partials/logo.php.twig' %}
                {% include 'partials/nav-primary.php.twig' %}
            </div>
        </header>
        <main id="site__main" role="main">
            <div class="container">
                {% block content %}{% endblock %}
            </div>

        </main>
        <footer id="site__footer">
            <div class="container">
                {% include 'partials/nav-footer.php.twig' %}
            </div>
        </footer>
    </div>
    <div class="preloader">
        <div class="preloader__loader"></div>
    </div>
    <script type="text/javascript" src="{{function('theme_js_path')}}bundle.min.js"></script>
    {% include 'partials/tracking-codes.php.twig' %}
</body>
</html>